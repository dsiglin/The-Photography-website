.cover_gradiant_diagonal {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 4;
    background: -moz-linear-gradient(45deg,rgba(28,28,28,0) 75%, rgba(28,28,28,.15) 90%, rgba(28,28,28,0.35) 100%);
    background: -webkit-gradient(linear,right top,right bottom,color-stop(75%,rgba(66,66,66,0)),color-stop(90%,rgba(28,28,28,.15)),color-stop(100%,rgba(28,28,28,0.35)));
    background: -webkit-linear-gradient(45deg,rgba(28,28,28,0) 75%,rgba(28,28,28,.15) 90%,rgba(28,28,28,0.35) 100%);
    background: -o-linear-gradient(45deg,rgba(28,28,28,0) 75%, rgba(28,28,28,.15) 90%, rgba(28,28,28,0.35) 100%);
    background: -ms-linear-gradient(45deg,rgba(28,28,28,0) 75%, rgba(28,28,28,.15) 90%, rgba(28,28,28,0.35) 100%);
    background: linear-gradient(4deg,rgba(28,28,28,0) 75%, rgba(28,28,28,.15) 90%, rgba(28,28,28,0.35) 100%);
