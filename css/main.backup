.cover_gradiant {
    position: absolute;
    bottom: 0;
    left: 0;
    top: 0;
    right: 0;
    z-index: 4;
/* Permalink - use to edit and share this gradient: http://colorzilla.com/gradient-editor/#424242+23,424242+23,1c1c1c+98&amp;0+57,0.91+100 */
    background: -moz-linear-gradient(top,rgba(66,66,66,0) 23%,rgba(28,28,28,.15) 75%,rgba(28,28,28,0.47) 90%,rgba(28,28,28,0.77) 100%);
/* FF3.6+ */
    background: -webkit-gradient(linear,left top,left bottom,color-stop(23%,rgba(66,66,66,0)),color-stop(75%,rgba(28,28,28,.15)),color-stop(90%,rgba(28,28,28,0.47)),color-stop(100%,rgba(28,28,28,0.77)));
/* Chrome,Safari4+ */
    background: -webkit-linear-gradient(top,rgba(66,66,66,0) 23%,rgba(28,28,28,.15) 75%,rgba(28,28,28,0.47) 90%,rgba(28,28,28,0.77) 100%);
/* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(top,rgba(66,66,66,0) 23%,rgba(28,28,28,.15) 75%,rgba(28,28,28,0.47) 90%,rgba(28,28,28,0.77) 100%);
/* Opera 11.10+ */
    background: -ms-linear-gradient(top,rgba(66,66,66,0) 23%,rgba(28,28,28,.15) 75%,rgba(28,28,28,0.47) 90%,rgba(28,28,28,0.77) 100%);
/* IE10+ */
    background: linear-gradient(to bottom,rgba(66,66,66,0) 23%,rgba(28,28,28,.15) 75%,rgba(28,28,28,0.47) 90%,rgba(28,28,28,0.77) 100%);
/* W3C */
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#00424242',endColorstr='#e81c1c1c',GradientType=0);
/* IE6-9 */
}

.cover_gradiant_diagonal {
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    width: 100%;
    height: 100%;
    z-index: 4;
/* Permalink - use to edit and share this gradient: http://colorzilla.com/gradient-editor/#1c1c1c+21,1c1c1c+100&amp;0+57,0.73+100 */
background: -moz-linear-gradient(45deg,  rgba(28,28,28,0) 21%, rgba(28,28,28,0) 57%, rgba(28,28,28,0.73) 100%); /* FF3.6-15 */
