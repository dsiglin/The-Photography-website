@charset  "utf-8";

/* CSS Document */
.html {
}

.body {
    margin: 0;
    padding: 0;
    font-size: 100%;
    background-color: #4c4c4c;
}

* {
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    -ms-box-sizing: border-box;
    -o-box-sizing: border-box;
    box-sizing: border-box;
}

/* //////////////////////////INTRO//////////////////////////// */
.instructions {
    position: absolute;
    width: 100%;
    top: 0;
    bottom: 0;
    z-index: 110;
}

.instructions_background {
    position: absolute;
    background-color: rgba(76,76,76,0.90);
    width: 100%;
    top: 0;
    bottom: 0;
    -webkit-filter: blur(10px) grayscale(100%) brightness(60%);
    -moz-filter: blur(10px) grayscale(100%) brightness(60%);
    -o-filter: blur(10px) grayscale(100%) brightness(60%);
    -ms-filter: blur(10px) grayscale(100%) brightness(60%);
    filter: blur(10px) grayscale(100%) brightness(60%);
    -moz-transition: all .4s linear;
    -o-transition: all .4s linear;
    -ms-transition: all .4s linear;
    -webkit-transition: all .4s linear;
    transition: all .4s linear;
}

.instructions_wrapper {
    top: 40%;
    -ms-transform: translateY(-50%) translateX(-50%);
    -webkit-transform: translateY(-50%) translateX(-50%);
    -moz-transform: translateY(-50%) translateX(-50%);
    -o-transform: translateY(-50%) translateX(-50%);
    transform: translateY(-50%) translateX(-50%);
    left: 50%;
    position: absolute;
    width: auto;
    text-align: middle;
    z-index: 300;
}

