background: linear-gradient(45deg,  rgba(28,28,28,0) 21%,rgba(28,28,28,0) 57%,rgba(28,28,28,0.73) 100%); /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#001c1c1c', endColorstr='#ba1c1c1c',GradientType=1 ); /* IE6-9 fallback on horizontal gradient */

}

