/* :::::: FULLPAGE.JS ADJUSTMENTS :::::: */
/*////////////////////////////////////////*/
.fp-slidesNav {
    position: absolute;
    z-index: 4;
    left: 50%;
    opacity: 1;
    margin-left: 9px !important;
}

.fp-slidesNav.bottom {
    bottom: 3px;
    left: 14px;
}

.fp-slidesNav.top {
    top: 17px;
}

.fp-slidesNav ul {
    margin: 0;
    padding: 0;
}

.fp-slidesNav ul li {
    display: block;
    width: 14px;
    height: 13px;
    margin: 3px;
    position: relative;
}

.fp-slidesNav ul li {
    display: inline-block;
}

.fp-slidesNav ul li a {
    display: block;
    position: relative;
    z-index: 1;
    width: 100%;
    height: 100%;
    cursor: pointer;
    text-decoration: none;
}

.fp-slidesNav ul li a.active span,.fp-slidesNav ul li:hover a.active span {
    height: 12px;
    width: 12px;
    margin: -6px 0 0 -6px;
    border-radius: 100%;
    background-color: #FF0;
    border: 2px solid #FF0;
}

.fp-slidesNav ul li a span {
    border-radius: 50%;
    position: absolute;
    z-index: 1;
    height: 8px;
    width: 8px;
    border: 0;
    left: 50%;
    top: 50%;
    margin: -4px 0 0 -4px;
    -webkit-transition: all .2s ease-in-out;
    -moz-transition: all .2s ease-in-out;
    -o-transition: all .2s ease-in-out;
    transition: all .2s ease-in-out;
    border: 1px solid #d2d2d2;
    background: transparent;
}

.fp-slidesNav ul li:hover a span {
    width: 12px;
    height: 12px;
    margin: -6px 0 0 -6px;
    border: 1px solid #FF0;
    background-color: #FF0;
}

.fp-controlArrow {
    position: absolute;
    z-index: 4;
    top: 50%;
    -webkit-translate: transformY(-50%);
    -moz-translate: transformY(-50%);
    -o-translate: transformY(-50%);
    translate: transformY(-50%);
    cursor: pointer;
    border-style: solid;
    margin-top: -38px;
    -webkit-transform: translate3d(0,0,0);
    -moz-transform: translate3d(0,0,0);
    -ms-transform: translate3d(0,0,0);
    -o-transform: translate3d(0,0,0);
    transform: translate3d(0,0,0);
}

.fp-controlArrow.fp-prev {
    left: 14px;
    width: 28px;
    height: 28px;
    border-radius: 50%;
    border: 2px solid #d2d2d2;
    z-index: 4;
    padding: 0px;
}

.fp-controlArrow.fp-prev:hover {
    border-color: #FF0;
    background-color: #FF0;
}

.fp-controlArrow.fp-prev:active {
    border-color: #FF0;
    background-color: transparent;
}

.fp-controlArrow.fp-prev::before {
    content: "<";
    font-family: 'Advent Pro', sans-serif;
    font-weight: 300;
    font-style: normal;
    font-size: 1.5rem;
    /*line-height: 1.25rem;*/
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    -webkit-transform: translateY(-50%);
    -moz-transform: translateY(-50%);
    -o-transform: translateY(-50%);
    -ms-transform: translateY(-50%);
    padding-left: 6px;
    text-decoration: none;
    color: #d2d2d2;
    transition: all .2s ease-in-out;
    -webkit-transition: all .2s ease-in-out;
    -moz-transition: all .2s ease-in-out;
    -o-transition: all .2s ease-in-out;
}

.fp-controlArrow.fp-next:hover::before {
    color: #4c4c4c;
    font-weight: 900;
    -webkit-transition: all .2s ease-in-out;
/** Chrome & Safari **/
    -moz-transition: all .2s ease-in-out;
/** Firefox **/
    -o-transition: all .2s ease-in-out;
/** Opera **/
    transition: all .2s ease-in-out;
}

.fp-controlArrow.fp-next:active::before {
    color: #FF0;
    -webkit-transition: all .2s ease-in-out;
/** Chrome & Safari **/
    -moz-transition: all .2s ease-in-out;
/** Firefox **/
    -o-transition: all .2s ease-in-out;
/** Opera **/
    transition: all .2s ease-in-out;
}

.fp-controlArrow.fp-next {
    right: 14px;
    width: 28px;
    height: 28px;
    border-radius: 50%;
    border: 2px solid #d2d2d2;
    z-index: 4;
}

.fp-controlArrow.fp-next:hover {
    border-color: #FF0;
    background-color: #FF0;
}


.fp-controlArrow.fp-next:active {
    border-color: #FF0;
    background-color: transparent;
}

.fp-controlArrow.fp-next::before {
    content: ">";
    font-family: 'Advent Pro', sans-serif;
    font-weight: 300;
    font-style: normal;
    font-size: 1.5rem;
    /*line-height: 1.25rem;*/
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    -webkit-transform: translateY(-50%);
    -moz-transform: translateY(-50%);
    -o-transform: translateY(-50%);
    -ms-transform: translateY(-50%);
    padding-left: 9px;
    text-decoration: none;
    color: #d2d2d2;
    transition: all .2s ease-in-out;
    -webkit-transition: all .2s ease-in-out;
    -moz-transition: all .2s ease-in-out;
    -o-transition: all .2s ease-in-out;
}

.fp-controlArrow.fp-prev:hover::before {
    color: #4c4c4c;
    font-weight: 900;
    -webkit-transition: all .2s ease-in-out;
/** Chrome & Safari **/
    -moz-transition: all .2s ease-in-out;
/** Firefox **/
    -o-transition: all .2s ease-in-out;
/** Opera **/
    transition: all .2s ease-in-out;
}

.fp-controlArrow.fp-prev:active::before {
    color: #FF0;
